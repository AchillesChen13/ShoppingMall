<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="entity.OrderDB" table="orderdetail">

        <id name="orderNo" type="int">
            <generator class="native"/>
        </id>
        <property name="status" type="java.lang.String"/>
        <property name="sumPrice" type="double"/>
        <property name="recipeName" type="java.lang.String"/>
        <property name="recipePhone" type="java.lang.String"/>
        <property name="recipeAddre" type="java.lang.String"/>
        <property name="orderTime" type="java.lang.String"/>
        <property name="goodsName" type="java.lang.String"/>
        <property name="num" type="double"/>
        <property name="userId" type="int" />
        <property name="commodityNo" type="java.lang.String"/>
    </class>

</hibernate-mapping>

