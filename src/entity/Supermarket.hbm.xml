<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="entity.Supermarket" table="supermarket">

        <id name="supId" type="int">
            <generator class="native"/>
        </id>
        <property name="supName" type="java.lang.String"/>
        <property name="supPassword" type="java.lang.String"/>
        <property name="supPhone" type="java.lang.String"/>
    </class>

</hibernate-mapping>

